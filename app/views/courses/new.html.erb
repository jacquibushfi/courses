<h1>New Course</h1>


 <%= form_for(@course) do |f| %>

    <%= render partial: 'layouts/form_errors', locals: { form_errors: @course } %>

    <%= f.label :title %>
    <%= f.text_field :title %>

    <%= f.label :description %>
    <%= f.text_field :description %>

    <%= f.label :category %>
    <%= f.select :category, ["Glass", "Jewelry"] %>
    
  <%= f.submit %>
<% end %>
