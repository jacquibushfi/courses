<%= form_for(@course) do |f| %>
  
  <p>
    <%= f.label :title %>
    <%= f.text_field :title %>
  </p>

  <p>
    <%= f.label :description %>
    <%= f.text_field :description %>
  </p>

  <p>
    <%= f.label :category %>
    <%= f.text_field :category %>
  </p>

  <%= link_to "Add Rating", new_enrollment_path, "btn btn-primary" %>

<% end %>